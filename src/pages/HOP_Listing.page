        <apex:define name="scripts">
            <apex:includeLightning />
            <apex:outputPanel id="lstLoadedScript" layout="block">
                <script>
                    var lstId = '{!lst.Id}';
                    var lstLoaded = {!lst.Id != null};
                </script>
            </apex:outputPanel>
            <script>
                var relatedShown = false;

                function onSFDTEventReceive(event) {
                    if (event) {
                        if (event.getParam('message') == 'View') {
                            var row = event.getParam('data');
                            window.location = '/apex/HOP_ApplicationView?id=' + row.Id +
                                              '&retUrl=' + encodeURIComponent(window.location.pathname + window.location.search);
                        }
                        if (event.getParam('message') == 'New') {
                            if ({!lst.Lottery_Status__c == 'Lottery Complete'}) {
                                alert('The Lottery is already Complete, you cannot Add any more Applications');
                                return;
                            }
                            window.location = '/apex/HOP_ApplicationForm?listingId=' + lstId +
                                              '&retUrl=' + encodeURIComponent(window.location.pathname + window.location.search);
                        }
                    }
                }

                function tryInitRelated() {
                    if (!relatedShown && lstLoaded) {
                        $('#liRelInfo').removeClass('disabled');

                        var whereClause = '{!myUser.Profile.UserLicense.Name}' == '{!COMMUNITY_LICENSE_NAME}'
                                            ? " AND Status__c != '{!DRAFT_APPLICATION}'"
                                            : '';

                        $Lightning.use("c:SalesforceDataTablesApp", function() {
                            $Lightning.createComponent(
                                "c:SalesforceDataTables",
                                {
                                    style: "bootstrap",
                                    objectName: "Application__c",
                                    fieldList: "Listing__c, Name, Last_Name__c, First_Name__c, Application_Submitted_Date__c",
                                    whereClause: "Listing__c = '" + lstId + "'" + whereClause,
                                    orders: [
                                        {
                                            'col': 'Name',
                                            'order': 'desc'
                                        }
                                    ],
                                    eventButtons: [
                                        {
                                            "label": "View",
                                            "select": true,
                                            "send": "row"
                                        },
                                        {
                                            "label": "New"
                                        }
                                    ],
                                    removeLookupHyperlinks: true
                                },
                                "SFDT_Applications",
                                function(cmp) {
                                    $A.eventService.addHandler({
                                        "event": "c:SalesforceDataTablesEvent",
                                        "handler" : onSFDTEventReceive
                                    });
                                    cmp.getElement().parentNode.classList.remove("slds-scope");
                                }
                            );
                        });

                        $Lightning.use("c:SalesforceDataTablesApp", function() {
                            $Lightning.createComponent(
                                "c:SalesforceDataTables",
                                {
                                    style: "bootstrap",
                                    objectName: "Open_Houses__c",
                                    fieldList: "Listing__c, Name, Date__c, Start_Time__c, End_Time__c",
                                    whereClause: "Listing__c = '" + lstId + "'",
                                    defaults: {'Listing__c' : lstId},
                                    removeLookupHyperlinks: true
                                },
                                "SFDT_OpenHouses",
                                function(cmp) {
                                    cmp.getElement().parentNode.classList.remove("slds-scope");
                                }
                            );
                        });

                        $Lightning.use("c:SalesforceDataTablesApp", function() {
                            $Lightning.createComponent(
                                "c:SalesforceDataTables",
                                {
                                    style: "bootstrap",
                                    objectName: "Information_Session__c",
                                    fieldList: "Listing__c, Name, Date__c, Start_Time__c, End_Time__c, Venue__c, Street_Address__c, City__c",
                                    whereClause: "Listing__c = '" + lstId + "'",
                                    defaults: {'Listing__c' : lstId},
                                    removeLookupHyperlinks: true
                                },
                                "SFDT_InformationSessions",
                                function(cmp) {
                                    cmp.getElement().parentNode.classList.remove("slds-scope");
                                }
                            );
                        });

                        relatedShown = true;
                    }
                }

                $(document).ready(function() {
                    tryInitRelated();
                });
            </script>
        </apex:define>
    </apex:composition>
</apex:page>